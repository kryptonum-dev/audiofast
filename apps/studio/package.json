{
  "name": "studio",
  "private": true,
  "version": "1.0.0",
  "main": "package.json",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "bun run sanity dev",
    "format": "bun run prettier --write .",
    "start": "bun run sanity start",
    "lint": "bun run eslint .",
    "build": "bun run sanity build",
    "deploy": "bun run sanity deploy",
    "type": "bun run sanity schema extract --enforce-required-fields && bun run sanity typegen generate",
    "check-types": "bun run tsc --noEmit",
    "postinstall": "bun run sanity exec scripts/cli-alert-for-data.ts"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/assist": "^4.3.2",
