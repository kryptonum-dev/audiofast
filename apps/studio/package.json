    "@sanity/vision": "^3.99.0",
    "lucide-react": "^0.539.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sanity": "^3.99.0",
    "sanity-plugin-icon-picker": "^4.0.0",
    "sanity-plugin-media": "^3.0.3",
    "slugify": "^1.6.6",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "@portabletext/block-tools": "^1.1.31",
    "@sanity/eslint-config-studio": "^5.0.2",
    "@sanity/schema": "^3.99.0",
