{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "globalEnv": [
    "SANITY_API_READ_TOKEN",
    "SANITY_API_WRITE_TOKEN",
    "VERCEL_URL",
    "VERCEL_PROJECT_PRODUCTION_URL",
    "VERCEL_ENV",
    "NODE_ENV",
    "EMBEDDINGS_INDEX_BEARER_TOKEN",
    "MAILCHIMP_API_KEY",
    "MAILCHIMP_SERVER_PREFIX",
    "NEXT_REVALIDATE_TOKEN",
    "AZURE_TENANT_ID",
    "AZURE_CLIENT_ID",
    "AZURE_CLIENT_SECRET",
    "MS_GRAPH_SENDER_EMAIL",
    "MS_GRAPH_REPLY_TO",
    "EDGE_CONFIG",
    "EDGE_CONFIG_ID",
    "VERCEL_API_TOKEN",
    "SANITY_WEBHOOK_SECRET"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**", ".sanity/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
