{
  "name": "audiofast",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "bun run turbo build",
    "dev": "bun run turbo dev",
    "lint": "bun run turbo lint",
    "check-types": "bun run turbo check-types",
    "format": "bun run prettier --write \"**/*.{ts,tsx,md}\"",
    "typegen": "cd apps/studio && bunx sanity schema extract && bunx sanity typegen generate --enforce-required-fields"
  },
  "devDependencies": {
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "prettier": "^3.6.0",
    "turbo": "^2.5.8",
    "typescript": "5.7.3"
  },
  "packageManager": "bun@1.1.42",
  "engines": {
    "node": ">=20"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "trustedDependencies": [
    "core-js-pure",
    "studio"
  ]
}
